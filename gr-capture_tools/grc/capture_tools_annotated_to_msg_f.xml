<?xml version="1.0"?>
<block>
  <name>annotated_to_msg_f</name>
  <key>capture_tools_annotated_to_msg_f</key>
  <category>[capture_tools]</category>
  <import>import capture_tools</import>
  <make>capture_tools.annotated_to_msg_f()
self.$(id).set_sync_word($syncword)</make>
  <callback>self.$(id).set_sync_word($syncword)</callback>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Sync word (list)</name>
    <key>syncword</key>
    <value></value>
    <type>int_vector</type>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>float</type>
  </sink>

  <sink>
    <name>en</name>
    <type>float</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->

  <source>
    <name>packets</name>
    <type>message</type>
  </source>

</block>
