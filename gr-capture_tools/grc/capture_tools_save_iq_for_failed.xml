<?xml version="1.0"?>
<block>
  <name>save_iq_for_failed</name>
  <key>capture_tools_save_iq_for_failed</key>
  <category>[capture_tools]</category>
  <import>import capture_tools</import>
  <make>capture_tools.save_iq_for_failed($length_to_save, $max_in_queue, $save_path, $history)
self.$(id).set_search_tag($search_tag)</make>
  <callback>self.$(id).set_search_tag($search_tag)</callback>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Length to save</name>
    <key>length_to_save</key>
    <value>1000</value>
    <type>int</type>
  </param>

  <param>
    <name>History</name>
    <key>history</key>
    <value>1</value>
    <type>int</type>
  </param>

  <param>
    <name>Search tag</name>
    <key>search_tag</key>
    <value>"address"</value>
    <type>string</type>
  </param>

  <param>
    <name>Max in queue</name>
    <key>max_in_queue</key>
    <value>20</value>
    <type>int</type>
  </param>

  <param>
    <name>Save path</name>
    <key>save_path</key>
    <value></value>
    <type>string</type>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <sink>
    <name>in_float_with_start_tag</name>
    <type>float</type>
    <optional>1</optional>
  </sink>

  <sink>
    <name>passfail</name>
    <type>message</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
</block>
